
/**
*Set priority levels between 1 - 5
*/
public static PriorityLevel(short p)
{
  int n1= 0;
  boolean Flag1= true;
  while(Flag1==true)
  {
    if(p==1)
    {
      n1=1;
    }if p=1 end
    if(p==2)
    {
      n1=2;
    }//if p=2 end
    if(p==3)
    {
      n1=3;
    }//if p=3 end
    if(p==4)
    {
      n1=4;
    }//if p=4 end
    if(p==5)
    {
      n1=5;
    }//if p= 5 end
  }//while true end
}//PriorityLevel end
